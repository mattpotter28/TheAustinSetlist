{
  "name": "theaustinsetlist",
  "version": "1.0.0",
  "description": "The Austin Setlist is a newsletter that provides music enthusiasts in Austin, Texas with weekly updates on new concerts announced and upcoming concerts in the area. Stay up-to-date with the vibrant music scene of Austin, the Live Music Capital of the World!",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js && http-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "npx prettier --write .",
    "lint": "npx prettier --check .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattpotter28/TheAustinSetlist.git"
  },
  "author": "Matthew Potter",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mattpotter28/TheAustinSetlist/issues"
  },
  "homepage": "https://github.com/mattpotter28/TheAustinSetlist#readme",
  "husky": {
    "hooks": {
      "pre-commit": "prettier --check . && git add -A ."
    }
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "http-server": "^14.1.1"
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "prettier": "3.0.0",
    "pretty-quick": "^3.1.3"
  }
}
